default: all

all: build_lib


build_lib: build_completed

build_completed: Makefile
	./build.sh
	touch build_completed

distclean:
	rm -f $(DOWNLOAD_FILE)* build_completed
	for dir in *; do if [ -d "$$dir" ]; then rm -rf $$dir; fi; done

clean:
	rm -f $(DOWNLOAD_FILE)*
	for dir in src; do if [ -d "$$dir" ]; then rm -rf $$dir; fi; done
